<%=render 'shared/title', title: 'Post List'%>
<% @posts.each do |post| %>
 <div class="col-md-8">
<div class="panel panel-default">
 <div class="panel-heading">
<p><h4><%= post.title %></h4></p> </div>  
<div class="panel-body"> 
    <p><%= post.content %></p>
<br/>
    <span>
       <%= link_to "show this post", post_path(post)%></span>
    </div>
  </div>
</div>
<% end %>



